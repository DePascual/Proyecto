<h2>Activities @ViewData["idUsu"]</h2>

<table id="tablaACtivities" class="table table-striped center" style="text-align:center; width:100%; height:100%">
    <tr>
        <th style="text-align:center">
            Nombre
        </th>
        <th style="text-align:center">
            Points
        </th>
    </tr>
    @{
        var recentWorkouts = (List<FITOCRACY.Models.Workouts>)ViewData["recentWorkouts"];
        foreach (var w in recentWorkouts)
        {
            <tr>
                <td>@w.Nombre</td>
                <td>@w.Puntos</td>
            </tr>
        }
    }
</table>
